#!/usr/bin/env bash

# IMPORT UTILS

source "$(dirname $0)"/../utils.sh

SCRIPTS_FOLDER=$(
  cd "$(dirname "$0")"/../..
  pwd -P
)

if [ -f ~/.aws/credentials ]; then
  echo -e $CYAN"[ AWS ACCOUNTS PROFILES ]"$COLOR_RESET
  cat ~/.aws/credentials | egrep -i "\[(.*)\]" | sed "s/\[/- /g" | sed "s/\]//g"
else
  echo "No AWS credentials found"
fi
